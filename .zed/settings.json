  "lsp": {
    "json-language-server": {
      "settings": {
        "json": {
          "schemas": [
            {
              "fileMatch": ["*/data/bangs.json", "*/data/kagi_bangs.json", "*/data/assistant_bangs.json"],
              "url": "./data/bangs.schema.json"
            }
          ]
        }
      }
    }
  }
}
